
@{
    Layout = "~/Views/Shared/Header.cshtml";
    ViewBag.Title = "Index";
    int id = ViewBag.ids;

}
<!doctype html>

<head>

</head>
<body>

    <!-- bradcam_area_start  -->
    <div class="bradcam_area breadcam_bg_2 bradcam_overlay">
        <div class="container">
            <div class="row">
                <div class="col-xl-12">
                    <div class="bradcam_text">
                        <h3>Confirmation Page</h3>
                        <p><a href="index.html">Tshirt/</a> confirmations</p>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- bradcam_area_end  -->
    <!-- expert_doctors_area_start -->

    <div class="expert_doctors_area doctor_page">
        <div class="container">
            <div class="row">

                <div class="col-12">
                    <h2 class="contact-title">Register With Us And Grow Up Your Bussiness</h2>
                </div>
                <div class="col-lg-8">
                    @using (Html.BeginForm())
                    {
                        @*<form class="form-contact contact_form" action="contact_process.php" method="post" id="contactForm" novalidate="novalidate">*@

                    <div class="row">
                        <div class="col-12">
                            <div class="form-group">
                                <input class="form-control" value="@id" name="idvalue" id="subject" type="hidden" onfocus="this.placeholder = ''" onblur="this.placeholder = 'Email Send otpcode'" placeholder="Email Send otpcode">
                            </div>
                        </div>
                        <div class="col-12">
                            <div class="form-group">
                                <input class="form-control" name="confirmation" id="subject" type="text" onfocus="this.placeholder = ''" onblur="this.placeholder = 'Email Send otpcode'" placeholder="Email Send otpcode">
                            </div>
                        </div>
                        <div class="col-12">
                            <div class="form-group">
                                <input class="form-control" name="username" id="subject" type="text" onfocus="this.placeholder = ''" onblur="this.placeholder = 'user Name'" placeholder="User Name">
                            </div>
                        </div>
                        <div class="col-12">
                            <div class="form-group">
                                <input class="form-control" name="password" id="subject" type="password" onfocus="this.placeholder = ''" onblur="this.placeholder = 'password'" placeholder="password">
                            </div>
                        </div>

                    </div>
                        <div class="form-group mt-3">
                            <button type="submit" class="button button-contactForm boxed-btn">confirm My Registration</button>
                        </div>
                        @*</form>*@
                    }
                </div>
                <div class="col-lg-3 offset-lg-1">
                    <div class="media contact-info">
                        <span class="contact-info__icon"><i class="ti-home"></i></span>
                        <div class="media-body">
                            <h3>Tshirt Company, Sri Lanka</h3>
                            <p>Narahenpita, Sri Lanka</p>
                        </div>
                    </div>
                    <div class="media contact-info">
                        <span class="contact-info__icon"><i class="ti-tablet"></i></span>
                        <div class="media-body">
                            <h3>+9471 21 25 166</h3>
                            <p>Mon to Fri 9am to 6pm</p>
                        </div>
                    </div>
                    <div class="media contact-info">
                        <span class="contact-info__icon"><i class="ti-email"></i></span>
                        <div class="media-body">
                            <h3>info@tshirt.lk</h3>
                            <p>Send your Errors Any Time!</p>
                        </div>
                    </div>
                </div>


            </div>
        </div>
    </div>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js"></script>
    <script>
        function submitform(thiss) {
            $('form').submit();
        }
    </script>

</body>
</html>