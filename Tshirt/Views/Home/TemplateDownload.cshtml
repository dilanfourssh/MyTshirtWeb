
@{
    Layout = "~/Views/Shared/Header.cshtml";
    var mydownloadfile = ViewBag.templatedownload;
}

<!DOCTYPE html>
<html>
<head>

    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link href="~/css/style.css" rel="stylesheet" />

    @*<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">*@
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>
</head>
<body>


   
    <div class="container">
        <div class="row" id="webtemplatetitle">
            <div class="col-md col-sm" style="border-radius:15px 15px 15px 15px;"><img src="~/img/orderimage/templatemain.png" style="width:100%; border-radius:15px 15px 15px 15px;" /></div>
        </div>
        <div class="row" id="webtemplatetitle" style="background-color:#f8f9fa; border-radius:10px 10px 10px 10px; padding:20px 20px 20px 20px;">
            <div class="col-md col-sm">
                <h4 style="color:green"> <i class="fa fa-download"></i> Premium Qulity Android Flutter Template ....</h4>
            </div>
        </div>
        <div class="row" id="imagerow" style="margin-top:30px;">
            <div class="col-sm">
                <div class="row" id="webtemplatetitle" style=" border-radius:10px 10px 10px 10px; padding:20px 20px 20px 20px;">
                    @*start*@
                    <div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
                        <div class="carousel-inner">
                            <div class="carousel-item active">
                                <img class="d-block w-100" src="~/img/orderimage/@mydownloadfile.imgname1" alt="First slide">
                            </div>
                            <div class="carousel-item">
                                <img class="d-block w-100" src="~/img/orderimage/@mydownloadfile.imgname2" alt="Second slide">
                            </div>
                            <div class="carousel-item">
                                <img class="d-block w-100" src="~/img/orderimage/@mydownloadfile.imgname3" alt="Third slide">
                            </div>
                        </div>
                        <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
                            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                            <span class="sr-only">Previous</span>
                        </a>
                        <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
                            <span class="carousel-control-next-icon" aria-hidden="true"></span>
                            <span class="sr-only">Next</span>
                        </a>
                    </div>
                    @*end carosal*@
                    <div class="col-md col-sm">

                        <h4 style="color:green"><i class="fa fa-cogs" aria-hidden="true"></i>  @mydownloadfile.tittle</h4>
                        <p style="font-family:Courier New; text-align:justify">
                            @mydownloadfile.largeDescription
                        </p>
                        @using (Html.BeginForm())
                        {
                            <input type="hidden" name="id" value="@mydownloadfile.id" />
                            <input type="hidden" name="zipfilename" value="@mydownloadfile.zipfilename" />
                            <button type="submit" class="btn btn-outline-success" style="width:100%">Download Template</button>
                        }
                    </div>
                </div>
            </div>


        </div>
    </div>





</body>
</html>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js"></script>
<script>
    function submitform(thiss) {
        $('form').submit();
    }
</script>